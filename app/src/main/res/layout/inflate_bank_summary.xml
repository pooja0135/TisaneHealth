<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.cardview.widget.CardView
            android:id="@+id/cardviewDetail"
            app:cardCornerRadius="@dimen/_5sdp"
            android:layout_margin="@dimen/_5sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_190sdp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Member ID"
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>
                        <TextView
                            android:id="@+id/tvMemberId"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Member Name"
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>
                        <TextView
                            android:id="@+id/tvMembername"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Admin Charges"
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>
                        <TextView
                            android:id="@+id/tvAdminCharges"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="TDS(Tax Deduction at  Source)"
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>
                        <TextView
                            android:id="@+id/tvTDS"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Payable Amount"
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>
                        <TextView
                            android:id="@+id/tvPayable"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Pan Card"
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>
                        <TextView
                            android:id="@+id/tvPancard"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Generate Date"
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>
                        <TextView
                            android:id="@+id/tvGenerateDate"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>

                    </LinearLayout>

                    <LinearLayout
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Transfer Date"
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>
                        <TextView
                            android:id="@+id/tvTransferDate"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/black"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_30sdp">
                        <TextView
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:id="@+id/tvBankDetail"
                            android:paddingTop="@dimen/_4sdp"
                            android:paddingBottom="@dimen/_4sdp"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Bank Details"
                            android:textSize="@dimen/_12sdp"
                            android:textColor="@color/white"
                            android:background="@drawable/rectangle_button"
                            />

                        <TextView
                            android:id="@+id/tvIncomeDetail"
                            android:paddingTop="@dimen/_4sdp"
                            android:paddingBottom="@dimen/_4sdp"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Income Details"
                            android:textSize="@dimen/_12sdp"
                            android:layout_marginLeft="@dimen/_15sdp"
                            android:textColor="@color/white"
                            android:background="@drawable/rectangle_button"/>


                    </LinearLayout>
                </LinearLayout>
                <RelativeLayout
                    android:orientation="horizontal"
                    android:layout_alignParentBottom="true"
                    android:layout_width="match_parent"
                    android:layout_marginBottom="@dimen/_50sdp"
                    android:layout_height="wrap_content">
                    <androidx.cardview.widget.CardView
                        android:visibility="gone"
                        android:id="@+id/cardviewBank"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_alignParentBottom="true"
                        app:cardCornerRadius="@dimen/_5sdp"
                        app:cardBackgroundColor="@color/grey_200"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:layout_margin="@dimen/_5sdp"
                            android:orientation="vertical"
                            android:layout_width="@dimen/_170sdp"
                            android:layout_height="wrap_content">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="Bank Name"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>
                                <TextView
                                    android:id="@+id/tvBankName"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="Beneficiary Name"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>
                                <TextView
                                    android:id="@+id/tvBeneficiaryName"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="Account\n Number"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>
                                <TextView
                                    android:id="@+id/tvAccountNumber"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>

                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="IFSC Code"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>
                                <TextView
                                    android:id="@+id/tvIfsc"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>

                            </LinearLayout>

                        </LinearLayout>
                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:visibility="gone"
                        android:layout_alignParentRight="true"
                        android:id="@+id/cardviewIncome"
                        android:layout_marginRight="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_20sdp"
                        android:layout_alignParentBottom="true"
                        app:cardCornerRadius="@dimen/_5sdp"
                        app:cardBackgroundColor="@color/grey_200"
                        android:layout_width="@dimen/_200sdp"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:layout_margin="@dimen/_5sdp"
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:gravity="center"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="BP Matching Income:"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>
                                <TextView
                                    android:id="@+id/tvMatchingIncome"
                                    android:gravity="center_vertical"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>

                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:gravity="center"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="Sponsor Income:"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>
                                <TextView
                                    android:id="@+id/tvDirectIncome"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>

                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:gravity="center"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="Turnover Income:"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>
                                <TextView
                                    android:id="@+id/tvRoiIncome"
                                    android:gravity="center_vertical"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:gravity="center"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="Leader Performance Income:"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>
                                <TextView
                                    android:id="@+id/tvLeaderIncome"
                                    android:gravity="center_vertical"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>

                            </LinearLayout>


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:gravity="center"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="Crowm Directship Income:"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>
                                <TextView
                                    android:id="@+id/tvCrownIncome"
                                    android:gravity="center_vertical"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10sdp"/>

                            </LinearLayout>





                        </LinearLayout>
                    </androidx.cardview.widget.CardView>
                </RelativeLayout>

            </RelativeLayout>

        </androidx.cardview.widget.CardView>

    </RelativeLayout>


</LinearLayout>
